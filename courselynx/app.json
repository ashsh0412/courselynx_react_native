{
  "expo": {
    "name": "courselynx",
    "slug": "courselynx",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.courselynx.courselynx",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-image-picker",
        {
          "cameraPermission": "We need access to your camera to take pictures and videos.",
          "photosPermission": "We need access to your photos to allow you to select media."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow CourseLynx to access your camera",
          "microphonePermission": "Allow CourseLynx to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "react-native-video",
        {
          "enableNotificationControls": true,
          "androidExtensions": {
            "useExoplayerRtsp": false,
            "useExoplayerSmoothStreaming": false,
            "useExoplayerHls": false,
            "useExoplayerDash": false
          }
        }
      ],
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "expo": {
      "assetBundlePatterns": ["**/*"]
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a826653f-f94e-48c0-8f34-d63848078278"
      }
    },
    "owner": "courselynx"
  }
}
